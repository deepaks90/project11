pipeline {
    agent none
    stages {
        stage('Run Tests') {
            parallel {
                stage('Test On Windows') {
                    agent {
                        label "windows"
                    }
                    steps {
                      "echo parallel executions test"
                    }
                   
                }
                stage('Test On Linux') {
                    agent {
                        label "linux"
                    }
                    steps {
                       "echo parallel executions test2"
                    }
                  
                }
            }
        }
    }
}
